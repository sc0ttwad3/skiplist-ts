{
  "name": "skiplist-ts",
  "version": "0.0.2",
  "description": "A fast SkipList with advanced features implemented in Typescript",
  "main": "lib/index",
  "types": "lib/index",
  "module": "lib/index",
  "scripts": {
    "clean": "shx rm -rf ./lib",
    "lint": "tslint src/**/*.ts",
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json -watch",
    "test": "jest"
  },
  "keywords": [
    "skiplist",
    "typescript",
    "data-structure"
  ],
  "author": "Scott Wade <sc0ttwad3@gmail.com> (http://sc0ttwad3.com)",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^20.0.7",
    "@types/node": "^8.0.24",
    "chalk": "^2.1.0",
    "jest": "^20.0.4",
    "shx": "^0.2.2",
    "ts-jest": "^20.0.10",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "typescript": "^2.6.0-dev.20170818"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sc0ttwad3/skiplist-ts.git"
  },
  "bugs": {
    "url": "https://github.com/sc0ttwad3/skiplist-ts/issues"
  },
  "homepage": "https://github.com/sc0ttwad3/skiplist-ts#readme"
}
